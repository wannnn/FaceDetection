<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.claire.facedetection.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/frag_main">
        <action
            android:id="@+id/to_faceDetectionFragment"
            app:destination="@id/faceDetectionFragment" />
    </fragment>

    <fragment
        android:id="@+id/faceDetectionFragment"
        android:name="com.claire.facedetection.facedetection.FaceDetectionFragment"
        android:label="FaceDetectionFragment"
        tools:layout="@layout/frag_face_detection"/>

</navigation>